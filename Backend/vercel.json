{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/v1/(.*)",
      "dest": "/src/routes/auth.routes.js"
    },
    {
      "src": "/api/user/v1/(.*)",
      "dest": "/src/routes/user.routes.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ],
  
  
}
